int t = 0;
int i,j,j0,x,y,t1,col;

// music(0)

void update() {
 t += 6;
}

void draw() {
 cls();
 // palt(0);
 for(i=1; i<=11; i++) {
  for(j0=0; j0<=7; j0++) {
    j = 7-j0;
    col = 7+j;
    t1 = t + i*30 - j*10;
    // x = cos(t0)*5;
    x = 0;
    y = 38 + j + cos(t1)/32;
    pal(7,col);
    spr(16+i, 8+i*8 + x, y, 8, 8);
  }
 }
 
 cursor(37,70);
 fcol(14);
 print("THIS IS ESPICO");

 cursor(34,80);
