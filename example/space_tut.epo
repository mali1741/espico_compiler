__nfo__
Space shooter tutorial
__src__
#define IMM_FLAG 8

actor ship = 0;
actor bullet = 8;
actor enemy = 20;
int numenemies = 0;
int points = 0;
int pc = 0;

struct star {
  int x,y,s;
};
star stars[128];

struct enemyvals {
  int m_x, m_y;
};

enemyvals evals[8]; 
actor cb_e = -1;

void explode(int x, int y) {
  partset(x,y,pc,3,4);
}

void cb_shipcoll(int n, int e) {
  // if collision with enemy (id 20-27)
  // and not IMM
  // set IMM, decr lives
  if (e >= 20 && e < 28) {
    if ((ship.flags & IMM_FLAG) == 0) {
      ship.flags = IMM_FLAG;
      ship.lives -= 1;
    }
  }
}

int fr = 0;
void cb_shipanim(int n, int f) {
  // animate ship
  if (f%6 < 3) {
    ship.sprite=1;
  } else {
    ship.sprite=2;
  }
  // blink ship
  if ((ship.flags & IMM_FLAG) && ((f & 7) < 4)) ship.sprite = 0;
}

actor cb_b = -1;

void cb_bulletcoll(int n, int e) {
  if (e >= 20 && e < 28) {
    cb_e = cbactor(e);
    points += 1;
    explode(cb_e.x,cb_e.y);
    cb_e.lives = 0;
    numenemies--;
  }
}

void cb_bulletanim(int n, int f) {
  // if event not IN_EVENT delete bullet
  if ((n & ACTOR_IN_EVENT) == 0) {
   cb_b = cbactor(n);
   cb_b.lives = 0;
  }
}

actor cb_e = -1;

void cb_enemyanim(int n, int f) {
  cb_e = cbactor(n);
  evals[cb_e-20].m_y += 1;
  cb_e.x = ((3*sin(cb_e.angle)) / 32) + void(evals[cb_e-20].m_x);
  cb_e.y = ((3*cos(cb_e.angle)) / 32) + void(evals[cb_e-20].m_y);

  cb_e.angle += (cb_e.refval < 0) ? (-8) : 8;
 
  if (cb_e.y > 150) {
    cb_e.lives = 0;
    numenemies--;
  }
}

void init() {
 arst(ALL_ACTORS);
 numenemies = 0;

 ship.sprite = 1;
 ship.x = 60;
 ship.y = 100;
 ship.hh = 4;
 ship.hw = 4; 
 ship.lives = 4;
 ship.flags = 0;
 ship.onanimate = (cb_shipanim);
 ship.oncollision = (cb_shipcoll);
 ship.refval = 0; // ship.t

 int i;
 for(i=0; i<128; i++) {
  stars[i].x = rnd(128);
  stars[i].y = rnd(128);
  stars[i].s = rnd(2)+1;
 }

 parttime(600,200);
 partdir(0,230,310,1);
 pc = partcolor(10,9,3,PARTICLE_SHRINK|PARTICLE_FRIC);
}

void respawn() {
  numenemies = rnd(6)+2;
  enemy = 20;
  int i;
  for(i = 0; i < numenemies; i++) {
    int d = -1;
    if (rnd(1)) d = 1;
    enemy.sprite = 17;
    enemy.x = -32;
    enemy.y = -32;
    enemy.onanimate = (cb_enemyanim);
    enemy.refval = d;
    enemy.angle = d * 72;
    enemy.lives = 1;
    evals[i].m_x = 16 + i*16;
    evals[i].m_y = -20 - i*8;
    enemy++;
  }
}

void draw_over() {
  fcol(11);
  cursor(46,60);
  print("GAME OVER");
}

int cooldown = 0;

void fire() {
 // add bullet to actors
 if (bullet.lives) return;
 bullet.lives = 1;
 bullet.sprite = 3;
 bullet.x = ship.x;
 bullet.y = ship.y;
 bullet.dx = 0;
 bullet.dy = -3;
 bullet.hh = 5;
 bullet.hw = 2;
 bullet.oncollision = (cb_bulletcoll);
 bullet.onanimate = (cb_bulletanim);
 // use next bullet slot
 bullet++;
 if (bullet > 15) bullet = 8;
}

void update() {
  int key;
  int i;

  key = btn(BTN_ALL);

  if (ship.lives <= 0) {
     if (key & KEY_B) init();
     return;
  }

  if (ship.flags & IMM_FLAG) {
    ship.refval += 1;
    if (ship.refval > 30) {
     ship.flags = 0;
     ship.refval = 0;
    }
  }

  for(i=0; i < 128; i++) {
    stars[i].y += stars[i].s;
    if (stars[i].y >= 128) {
      stars[i].y = 0;
      stars[i].x = rnd(128);
    }
  }

  atstcoll();

  if (numenemies <= 0) respawn();
 
  amove(ALL_ACTORS);

  if (cooldown > 0) cooldown--;

  if ((key & KEY_LEFT) && (ship.x > 1)) ship.x -= 2;
  if ((key & KEY_RIGHT) && (ship.x < 127)) ship.x += 2;
  if ((key & KEY_UP) && (ship.y > 1)) ship.y -= 2;
  if ((key & KEY_DOWN) && (ship.y < 127)) ship.y += 2;
  if ((key & KEY_A) && (cooldown == 0)) {
	fire();
	cooldown = 4;
  }
}


void draw() {
  int i = 0;
  cls();
  fcol(7);
  for(i=0; i<128; i++) {
    pset(stars[i].x,stars[i].y);
  }
 
  cursor(5,8);
  fcol(11);
  printn(points);

  if (ship.lives <= 0) {
    draw_over();
  } else {
    // explosions using particles
    partdraw();
    // draw all actors
    adraw(ALL_ACTORS);
  }

  // lives indicators
  for(i=1; i<=4; i++) {
    if (i<=ship.lives)
      spr(33,80+6*i,3,8,8);
    else
      spr(34,80+6*i,3,8,8);
  }
}
