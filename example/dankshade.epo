//////////////////////////////-
// light object
//////////////////////////////-

struct light {
  int px, py, r;
  fixed bri;
};

light lgt;

void lgt_new(int x, int y) {
 lgt.px = x;
 lgt.py = y;
 lgt.bri = 1.0;
 lgt.r = LIGHT_MAXR*lgt.bri;
 fl_light(lgt.bri);
}

void lgt_update() {
  int change = 0;
  //anchor to avatar
  // self.pos=ply.pos
  //controlling the light
  if (btn(BTN_UP) && lgt.py > 0) {
    lgt.py -= 1;
  }
  if (btn(BTN_DOWN) && lgt.py < 127) {
    lgt.py += 1;
  }
  if (btn(BTN_LEFT) && lgt.px > 0) {
    lgt.px -= 1;
  }
  if (btn(BTN_RIGHT) && lgt.px < 127) {
    lgt.px += 1;
  }
  
  if (btn(4) && lgt.bri>0.2) {
   lgt.bri-=0.02;
   change = 1;
  }
  if (btn(5) && lgt.bri<1.0) {
   lgt.bri+=0.02;
   change = 1;
  }
  if (change) {
    lgt.r = lgt.bri*LIGHT_MAXR;
    fl_light(lgt.bri);
  }
}
 
void lgt_extents() {
  bcol(0);
  rectfill(lgt.px-lgt.r-8,lgt.py-lgt.r-8,lgt.px+lgt.r+8,lgt.py+lgt.r+8);
  clip(lgt.px-lgt.r,lgt.py-lgt.r,2*lgt.r,2*lgt.r);
}
 
void lgt_apply() {
  int xs, xe, y, l, brkpi, my, fy, fx, lx;
  brkpi = (lgt.r)*BRKPTS_SIZE;
  my = lgt.py+lgt.r;
  my = (my < 127) ? my : 127;
  
  fy = lgt.py-lgt.r;
  if (fy < 0) {
	brkpi += fy*BRKPTS_SIZE;
	fy = 0;
  }
  scraddr = SCREEN_MEMMAP + (fy << 6);

  fx = lgt.px-lgt.r;
  fx = (fx < 0) ? 0 : fx;
  lx = lgt.px+lgt.r;
  lx = (lx < 127) ? lx : 127;

  for(y=fy; y<=my; y++) {
    xs = fx;
	
    for(l=light_brkpts[brkpi+BRKPTS_HLV]; l>=light_brkpts[brkpi+BRKPTS_LLV]+1; l--) {
      xe=lgt.px-light_brkpts[brkpi+l-1];
      if (xe < 0) continue;
      if (l == 6) {
        // fill black
	bcol(0);
        rectfill(xs,y,xe-1,y);
      } else {
        // shade
        fl_blend(xs,xe-1,y,l);
      }
      xs = xe;
    }

    for(l=light_brkpts[brkpi+BRKPTS_LLV]; l<light_brkpts[brkpi+BRKPTS_HLV]; l++) {
      xe=lgt.px+light_brkpts[brkpi+l];
      if (xe >= 127) break;
      if (l == 6) {
        // fill black
	bcol(0);
        rectfill(xs,y,xe,y);
      } else {
        // shade
        fl_blend(xs,xe,y,l);
      }
      xs = xe+1;
    }

    // last segment, last breakpoint to right bound
    if (l == 6) {
	bcol(0);
        rectfill(xs-1,y,lx,y);
    } else {
        fl_blend(xs,lx,y,l);
    }

    brkpi += (y >= lgt.py) ? (BRKPTS_SIZE) : (-BRKPTS_SIZE);
    scraddr += 64;
  }
}

//////////////////////////////-
// initialization
//////////////////////////////-

void init() {
 init_blending(LIGHT_NUMLVS);
 // init_palettes(16);
  
 lgt_new(64,64);
}

//////////////////////////////-
// main loop
//////////////////////////////-

void update() {
 // let all objects update
 lgt_update();
}

void draw() {
 // bcol(0);
 // cls();
 palrst();
 palt(0);
 // clip to lit rectangle
 lgt_extents();
 // background from level map
 map(0,0,0,0,16,16,0);
 // apply lighting to all that
 lgt_apply();
 // fcol(7);
 // line(0,64,127,64);
}
