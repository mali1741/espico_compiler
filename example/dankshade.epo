// dank tombs tech demo
// by jakub wasilewski

// positively not a game yet

// press üÖæÔ∏è/‚ùé to manipulate
// light radius, d-pad to
// pan around

//////////////////////////////-
// basic fills
//////////////////////////////-

// a fill function is just
// a function(x1,x2,y) that
// draws a horizontal line

// returns a fill function
// that draws a solid color
// void fl_color(x1,x2,c) {
//   rectfill(x1,y,x2,y,c)
// }

// used as fill function
// for ignored areas
// void fl_none() {
// }

//////////////////////////////-
// not so fast blend fill :P
//////////////////////////////-

// 256 * 5
char palconv[1280];
char *addr;

// sets up everything
// blend_line will need
void init_blending(int nlevels) {
 // tabulate sqrt() for speed
 // _sqrt={}
 // for i=0,4096 do
 // _sqrt[i]=sqrt(i)
 // end

 // populate look-up tables
 // for blending based on
 // palettes in sprite mem
 int lv,c1,c2;
 char topl;
 nlevels -= 1;
 addr = palconv;
 for(lv=0; lv<nlevels; lv++) {
  // light luts are stored in
  // memory directly, table
  // indexing is costly
  for(c1=0; c1<16; c1++) {
   topl=sget(lv,c1) << 4;
   for(c2=0; c2<16; c2++) {
    *addr = topl+sget(lv,c2);
    addr++;
   }
  }
 } 
}
int lutpal;
int scraddr;

void fl_blend(int x1,int x2,int y,int l) {
  if (l < 2) return;
  lutpal = void(palconv)+((l-1) << 8);
  // left and right odd pixels brutally rounded
  x1 -= (x1 & 1);
  memconv(scraddr+(x1 >> 1),lutpal,((x2-x1+1) >> 1));
}

//////////////////////////////-
// lighting
//////////////////////////////-

// determines how far each
// level of light reaches
// this is distance *squared*
// due to the ordering here,
// light level 1 is the
// brightest, and 6 is the
// darkest (pitch black)
#define LIGHT_MAXR  42
#define LIGHT_NUMLVS 6

int light_rng[] = {-1000,420,756,1092,1428,1764};
// 10*42,18*42,
// 26*42,34*42,
// 42*42,

// special "guard" value
// to ensure nothing can be
// light level 0 without ifs
// light_rng[0]=-1000

//  this is our "light" fill
// function.
//  it operates by dimming
// what's already there.
//  each horizontal line
// is drawn by multiple
// calls to another fill
// function handling
// the correct light level
// for each segment.
// light_fills={
//  fl_none,fl_blend(2),fl_blend(3),
//  fl_blend(4),fl_blend(5),fl_color(0)
// }
char light_brkpts[336]; // (light_numlvs+2)*light_maxr
// brkpts={}
#define BRKPTS_SIZE 8
#define BRKPTS_HLV 6
#define BRKPTS_LLV 7

void fl_light(fixed brightness) {
  int y, ysq, brp, srng, lv, brkpi, radius;
  fixed brimul;
  radius = LIGHT_MAXR*brightness;
  brimul = brightness*brightness;
  brkpi = 0;
  for(y=0; y<=radius; y++) {
    ysq = y*y;

    lv = 5;
    light_brkpts[brkpi+BRKPTS_HLV] = lv+1;  // highest level
    while (lv>=1) {
      brp = int(light_rng[lv]*brimul)-ysq;
      if (brp > 0) {
        light_brkpts[brkpi+lv] = sqrt(brp);
      } else {
        // llv=lv+1;
        break;
      }
      lv--;
    }
    light_brkpts[brkpi+BRKPTS_LLV] = lv+1;  // lowest level

    brkpi += BRKPTS_SIZE; // light_numlvs+2;
  }
}

